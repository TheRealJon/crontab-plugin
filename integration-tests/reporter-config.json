{
  "reporterEnabled": "mocha-junit-reporter, mochawesome",
  "mochaJunitReporterReporterOptions": {
    "mochaFile": "./artifacts/junit_cypress-[hash].xml",
    "toConsole": false
  },
  "mochawesomeReporterOptions": {
    "reportDir": "./artifacts/",
    "reportFilename": "cypress_report",
    "overwrite": false,
    "html": false,
    "json": true
  }
}
